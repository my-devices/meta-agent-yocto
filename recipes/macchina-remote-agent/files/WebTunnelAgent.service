[Unit]
Description=WebTunnelAgent
After=syslog.target network.target
StartLimitIntervalSec=0

[Service]
Type=forking
Restart=always
RestartSec=1
User=root
ExecStart=@BINDIR@/WebTunnelAgent --config=@SYSCONFDIR@/WebTunnelAgent.properties --pidfile=@LOCALSTATEDIR@/run/WebTunnelAgent.pid --daemon
PIDFile=@LOCALSTATEDIR@/run/WebTunnelAgent.pid

[Install]
WantedBy=multi-user.target
